{
  "realm": "conference-realm",
  "enabled": true,
  "sslRequired": "external",
  "registrationAllowed": false,
  "loginWithEmailAllowed": true,
  "duplicateEmailsAllowed": false,
  "resetPasswordAllowed": true,
  "editUsernameAllowed": false,
  "bruteForceProtected": true,
  "roles": {
    "realm": [
      {
        "name": "ADMIN",
        "description": "Administrator role with full access"
      },
      {
        "name": "USER",
        "description": "Standard user role"
      }
    ]
  },
  "clients": [
    {
      "clientId": "conference-client",
      "name": "Conference Management Client",
      "enabled": true,
      "publicClient": false,
      "secret": "conference-client-secret",
      "redirectUris": [
        "http://localhost:8888/*",
        "http://localhost:4200/*"
      ],
      "webOrigins": [
        "http://localhost:8888",
        "http://localhost:4200",
        "*"
      ],
      "standardFlowEnabled": true,
      "directAccessGrantsEnabled": true,
      "protocol": "openid-connect"
    },
    {
      "clientId": "angular-frontend",
      "name": "Angular Frontend Application",
      "enabled": true,
      "publicClient": true,
      "redirectUris": [
        "http://localhost:4200/*"
      ],
      "webOrigins": [
        "http://localhost:4200",
        "*"
      ],
      "standardFlowEnabled": true,
      "directAccessGrantsEnabled": true,
      "protocol": "openid-connect"
    }
  ],
  "users": [
    {
      "username": "admin",
      "email": "admin@conference.com",
      "enabled": true,
      "emailVerified": true,
      "firstName": "Admin",
      "lastName": "User",
      "credentials": [
        {
          "type": "password",
          "value": "admin123",
          "temporary": false
        }
      ],
      "realmRoles": [
        "ADMIN",
        "USER"
      ]
    },
    {
      "username": "user",
      "email": "user@conference.com",
      "enabled": true,
      "emailVerified": true,
      "firstName": "Standard",
      "lastName": "User",
      "credentials": [
        {
          "type": "password",
          "value": "user123",
          "temporary": false
        }
      ],
      "realmRoles": [
        "USER"
      ]
    }
  ]
}